<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.gn.module.main.FXMLlinechartController">
   <children>
      <AnchorPane id="AnchorPane" layoutX="10.0" layoutY="10.0" prefHeight="400.0" prefWidth="600.0">
         <children>
            <BarChart fx:id="barchart" layoutX="14.0" layoutY="53.0" prefHeight="333.0" prefWidth="500.0">
               <xAxis>
                  <CategoryAxis label="Nombre des clients" side="BOTTOM" fx:id="x">
                     <tickLabelFont>
                        <Font name="System Bold" size="16.0" />
                     </tickLabelFont></CategoryAxis>
               </xAxis>
               <yAxis>
                  <NumberAxis fx:id="y" label="Nombre rendez-vous" side="LEFT">
                     <tickLabelFont>
                        <Font name="System Bold" size="8.0" />
                     </tickLabelFont></NumberAxis>
               </yAxis>
            </BarChart>
            <Button layoutX="252.0" layoutY="16.0" mnemonicParsing="false" onAction="#loadst" prefHeight="25.0" prefWidth="75.0" style="-fx-background-color: #82B1FF;" text="load" />
            <Button layoutX="14.0" layoutY="16.0" mnemonicParsing="false" onAction="#retourmenu" prefHeight="25.0" prefWidth="101.0" style="-fx-background-color: #84FFFF;" text="RetourMenu" />
            <Button layoutX="447.0" layoutY="16.0" mnemonicParsing="false" onAction="#reto" prefHeight="25.0" prefWidth="121.0" style="-fx-background-color: #84FFFF;" text="Retour rendez-vous" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
